#
# Makefile for musserver 1.4
#

CC	= gcc
CFLAGS	= -I. -Wall -O2 -m32
LDFLAGS	=
#LDFLAGS	= -static

#############################################
# Nothing below this line should be changed #
#############################################

O=../../../objs/musserv

all: ${O}/musserver

${O}/musserver: \
	${O}/musserver.o \
	${O}/readwad.o \
	${O}/playmus.o \
	${O}/sequencer.o
	${CC} ${CFLAGS} ${LDFLAGS} \
	${O}/musserver.o \
	${O}/readwad.o \
	${O}/playmus.o \
	${O}/sequencer.o -o ${O}/musserver

clean:
	rm -f ${O}/*

${O}/%.o: %.c
	${CC} ${CFLAGS} -c $< -o $@
